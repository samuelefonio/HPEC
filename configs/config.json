{
    "device": "cpu",
    "dataset": "mnist",
    "grad": true,
    "validation": false,
    "output_dim": 2,
    "batch_size": 64,
    "epochs": 10,
    "model": "resnet18",
    "temperature": 0.1,
    "clipping": 1,
    "prototypes_ray":0.3,
    "K":0.1,
    "margin":0.01,
    "optimizer": {
        "name": "rsgd",
        "learning_rate": 1e-1,
        "weight_decay": 5e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [60, 140, 180], 
        "entity": 0.2
    }
    
}

