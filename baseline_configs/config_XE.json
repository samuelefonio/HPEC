{
    "device": "cpu",
    "dataset": "mnist",
    "method": "XE",
    "validation": false,
    "output_dim": 10, 
    "batch_size": 128,
    "epochs": 5,
    "temperature": 1,
    "clipping": 1,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-1,
        "weight_decay": 5e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [60, 120, 160], 
        "entity": 0.2
    }
}
