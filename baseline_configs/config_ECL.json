{
    "device": "cpu",
    "dataset": "mnist",
    "method": "ECL",
    "validation": false,
    "output_dim": 8,
    "batch_size": 128,
    "epochs": 5,
    "temperature": 0.1,
    "clipping": 1,
    "optimizer": {
        "name": "sgd",
        "learning_rate": 1e-1,
        "weight_decay": 1e-4,
        "momentum": 0.9
    },
    "lr_scheduler": {
        "steps": [60, 140, 160], 
        "entity": 0.2
    }
    
}
